{
    "scopeName": "source.molang",
    "name": "Molang",
    "patterns": [
        {
            "match": "\\b(query|q)\\.[a-zA-Z_][\\w.]*\\b",
            "name": "support.function.query.molang"
        },
        {
            "match": "\\b(math|variable|context|temp|animation|geometry|struct)\\.[a-zA-Z_][\\w.]*\\b",
            "name": "support.function.namespace.molang"
        },
        {
            "match": "\\b(true|false|null)\\b",
            "name": "constant.language.boolean.molang"
        },
        {
            "match": "-?\\b\\d+(\\.\\d+)?\\b",
            "name": "constant.numeric.molang"
        },
        {
            "match": "[+\\-*/%=!<>|&^]+",
            "name": "keyword.operator.molang"
        },
        {
            "match": "[\\[\\]\\(\\){}]",
            "name": "punctuation.brackets.molang"
        },
        {
            "match": "'(?:\\\\'|[^'])*'",
            "name": "string.quoted.single.molang"
        }
    ],
    "repository": {},
    "injectionSelector": "L:source.molang"
}